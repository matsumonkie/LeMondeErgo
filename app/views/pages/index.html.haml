%section.premiere 
  = render partial: 'premiere', object: @page.premiere
- @page.sectionsAndColors.each do |section, color|
  %section
    .row
      .span10.offset1.section-container
        %h1.text-center{:style => "color:#{color}; border-top: thick solid #{color};"}= section.header
        = render partial: 'main_article', object: section.main_article
        = render partial: 'article', object: section.articles
